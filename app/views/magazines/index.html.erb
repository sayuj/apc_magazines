<p id="notice"><%= notice %></p>

<h1>Listing Magazines</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @magazines.each do |magazine| %>
      <tr>
        <td><%= link_to magazine.name, magazine_articles_path(magazine) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
